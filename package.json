  "author": "Performance Analyzer Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "chart.js": "^3.9.1",
    "react-chartjs-2": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.0.0",
    "jest": "^29.0.0"
  },
  "files": [
    "src",
    "dist",
    "docs",
    "examples",
    "README.md",
    "LICENSE"
