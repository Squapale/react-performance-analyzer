{
  "name": "performance-analyzer-lib",
  "version": "1.0.0",
  "description": "A comprehensive performance analysis library for React applications (Core - MIT License)",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./core": {
      "import": "./src/core/index.js",
      "require": "./src/core/index.js"
    },
    "./components": {
      "import": "./src/components/index.js",
      "require": "./src/components/index.js"
    },
    "./testing": {
      "import": "./src/testing/index.js",
      "require": "./src/testing/index.js"
    },
    "./utils": {
      "import": "./src/utils/index.js",
      "require": "./src/utils/index.js"
    }
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "jest",
    "lint": "eslint src"
  },
  "keywords": [
    "performance",
    "react",
    "monitoring",
    "analysis",
    "optimization",
    "benchmarking",
    "free",
    "open-source",
    "mit-license"
